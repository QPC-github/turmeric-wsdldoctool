<?xml version="1.0" encoding="UTF-8"?><wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:tns="http://www.ebaypartnernetwork.com/adcommerce/v1/services" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/" name="CampaignService" targetNamespace="http://www.ebaypartnernetwork.com/adcommerce/v1/services">
	<wsdl:types>
		<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.ebaypartnernetwork.com/adcommerce/v1/services">


			<xs:complexType name="BaseCampaignResponse">
				<xs:annotation>
					<xs:documentation>
						Base response container for all Campaign Service calls.
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseAdCommerceResponse"/>
				</xs:complexContent>
			</xs:complexType>


			<xs:complexType name="CampaignResponse">
				<xs:annotation>
					<xs:documentation>
						Container for the campaign returned by the request. A campaign consists
						of a set of AdGroups, ads, and keywords. A campaign can also be associated
						with a specific set of eBay categories.
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseCampaignResponse">
						<xs:sequence>
							<xs:element name="campaign" type="tns:Campaign">
								<xs:annotation>
									<xs:documentation>
										Container of data pertaining to the associated campaign.
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>addCampaign</CallName>
											<CallName>deleteCampaign</CallName>
											<CallName>getCampaign</CallName>
											<CallName>getCampaigns</CallName>
											<CallName>updateCampaign</CallName>
											<Returned>Always</Returned>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>


			<xs:complexType name="BulkCampaignResponse">
				<xs:annotation>
					<xs:documentation>
						Container for the campaigns returned by the getCampaigns call.
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseCampaignResponse">
						<xs:sequence>
							<xs:element name="campaignList" type="tns:CampaignList">
								<xs:annotation>
									<xs:documentation>
										Container for the set of campaigns returned by getCampaigns.
									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>getCampaigns</CallName>
											<Returned>Always</Returned>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>


			<xs:complexType name="CampaignList">
				<xs:annotation>
					<xs:documentation>
						Container for the set of campaigns returned by getCampaigns.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="campaign" type="tns:Campaign">
						<xs:annotation>
							<xs:documentation>
								Container for the details of an ad campaign. Not returned if
								the associated campaign list has no current ad campaigns.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>getCampaigns</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>


			<xs:complexType name="Campaign">
				<xs:annotation>
					<xs:documentation>
						Container for the data related to a single ad campaign.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="0" name="campaignId" type="xs:long">
						<xs:annotation>
							<xs:documentation>
								eBay-assigned identification number
								associated with the respective ad
								campaign. Use this ID number to specify
								the ad campaigns you want to retrieve,
								update, or delete.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								User-supplied description of the
								associated ad campaign.
							</xs:documentation>
							<xs:appinfo>
								<MaxLength>[??]</MaxLength>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>updateCampaign</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="endDate" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>
								Specifies the time, in GMT format, that
								you want the ad campaign to end. If you
								set an end date, the campaign
								automatically becomes inactive on the
								date you specify. The campaign does not
								get deleted, so it is possible to
								restart the campaign by updatng the
								campaign with a new end date. See the
								datetime type for details on GMT. &lt;br
								/&gt; &lt;br /&gt; You are not required
								to specify a campaign end date. If you
								do not specify an end date, the the ads
								in that campaign will remain active and
								eligible for display every day, subject
								to your daily budget limits.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>updateCampaign</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="maxDailyBudget" type="xs:double">
						<xs:annotation>
							<xs:documentation>
								The maximum daily spend for the
								campaign. This budget limit covers every
								ad group contained within the associated
								campaign. You can modify the daily
								budget for a campaign any time--your
								changes will take effect on the next
								day, starting at [Midnight GMT??].
								&lt;br /&gt;&lt;br /&gt; The currency
								for the value specified is converted to
								currency that is associated with the
								country specified in the campaign
								target. [List ??] &lt;br /&gt;&lt;br
								/&gt; &lt;span
								class="tablenote"&gt;&lt;strong&gt;Note:&lt;/strong&gt;
								It is possible for the daily spend for
								an ad campaign to exceed the amount
								specified in maxDailyBudget. Budgets are
								targets and at times your actual daily
								spend may slightly exceed your daily
								budget settings. &lt;/span&gt;
							</xs:documentation>
							<xs:appinfo>
								<Default/>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>updateCampaign</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="name" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								The descriptive name that you want to
								give the ad campaign. The name should be
								descriptive, but short enough to use in
								the report tables.
							</xs:documentation>
							<xs:appinfo>
								<MaxLength>50</MaxLength>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>updateCampaign</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="startDate" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>
								Specify the time, in GMT, that you want
								the ad to begin. See the datetime type
								for details on GMT.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>updateCampaign</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="status" type="xs:token">
						<xs:annotation>
							<xs:documentation>
								Define the status of the ad campaign,
								either &lt;code&gt;active&lt;/code&gt;
								or &lt;code&gt;pause&lt;/code&gt;
								(inactive). You can use this switch to
								toggle the ad campaign on and off
								without modifying the start and end
								dates.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>updateCampaign</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="target" type="tns:Target">
						<xs:annotation>
							<xs:documentation>
								Specifies the country and language in
								which you want the ad to appear.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>updateCampaign</CallName>
									<RequiredInput>
										Conditionally
									</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="syndication" type="xs:boolean">
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:token">
						<xs:annotation>
							<xs:documentation>
								This field is deprecated and no longer does anything
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>


			<xs:complexType name="Target">
					<xs:annotation>
						<xs:documentation>
							Specifies the country and language to which the campaign is targeted.
						</xs:documentation>
					</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="0" name="country" type="xs:token">
						<xs:annotation>
							<xs:documentation>
								Specifies the country to which you want to target the ad campaign.
								Valid country values are:
								&lt;ul&gt;
									&lt;li&gt;Austrialia&lt;/li&gt;
									&lt;li&gt;Canada&lt;/li&gt;
									&lt;li&gt;France&lt;/li&gt;
									&lt;li&gt;Germany&lt;/li&gt;
									&lt;li&gt;Italy&lt;/li&gt;
									&lt;li&gt;United Kingdom&lt;/li&gt;
									&lt;li&gt;United States&lt;/li&gt;
								&lt;/ul&gt;
								Use the two-letter ISO 3166 country code to indicate the country in which
								you want the campaign to run. For English names that correspond to each
								country code (e.g., KY="Cayman Islands"), see the following ISO site:
								&lt;br /&gt; &lt;a href=
								"http://www.iso.ch/iso/en/prods-services/iso3166ma/02iso-3166-code-lists/list-en1.html"
								&gt;http://www.iso.ch/iso/en/prods-services/iso3166ma/02iso-3166-code-lists/list-en1.html
								&lt;/a&gt;.
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>updateCampaign</CallName>
									<RequiredInput>Conditionally</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="language" type="xs:token">
						<xs:annotation>
							<xs:documentation>
								Specifies the language in which the ad will run. Valid language
								values are:
								&lt;ul&gt;
									&lt;li&gt;Austrialia - English&lt;/li&gt;
									&lt;li&gt;Canada - English, Frence&lt;/li&gt;
									&lt;li&gt;France - French&lt;/li&gt;
									&lt;li&gt;Germany - German&lt;/li&gt;
									&lt;li&gt;Italy - Italian&lt;/li&gt;
									&lt;li&gt;United Kingdom - English&lt;/li&gt;
									&lt;li&gt;United States - English&lt;/li&gt;
								&lt;/ul&gt;
								[Is there a Language code a-la Country Code??]
							</xs:documentation>
							<xs:appinfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<RequiredInput>Yes</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>updateCampaign</CallName>
									<RequiredInput>Conditionally</RequiredInput>
								</CallInfo>
								<CallInfo>
									<CallName>addCampaign</CallName>
									<CallName>deleteCampaign</CallName>
									<CallName>getCampaign</CallName>
									<CallName>getCampaigns</CallName>
									<CallName>updateCampaign</CallName>
									<Returned>Conditionally</Returned>
								</CallInfo>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:complexType>


			<!--  calls  -->
			<xs:element name="addCampaignRequest">
				<xs:annotation>
					<xs:documentation>

					</xs:documentation>
					<xs:appinfo>
						<Summary>
							Request to add a campaign to an account. Campaigns contain a budget, start
							and end dates, a target (country and ad language), daily budget, and a name
							and description.
						</Summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="campaign" type="tns:Campaign">
							<xs:annotation>
								<xs:documentation>
									Container for the ad campaign. Campaigns contain a budget, start and
									end dates, and a target (country and ad language).
								</xs:documentation>
								<xs:appinfo>
									<CallInfo>
										<CallName>addCampaign</CallName>
										<RequiredInput>Yes</RequiredInput>
									</CallInfo>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="addCampaignResponse" type="tns:CampaignResponse"/>


			<xs:element name="deleteCampaignRequest">
				<xs:annotation>
					<xs:documentation>
						Deletes the campaign specified by the campaignId input by the user.
						&lt;br /&gt;&lt;br /&gt;
						Category campaigns cannot be deleted. eBay does not return an
						error if you attempt to delete an already deleted campaign.
					</xs:documentation>
					<xs:appinfo>
						<Summary>
							Deletes the campaign specified by the campaignId input by the user.
						</Summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="campaignId" type="xs:long">
							<xs:annotation>
								<xs:documentation>
									Specify the ad campaign you want to delete using the campaignId number
									that eBay returned when you created the ad campaign.
								</xs:documentation>
								<xs:appinfo>
									<CallInfo>
										<CallName>deleteCampaign</CallName>
										<RequiredInput>Yes</RequiredInput>
									</CallInfo>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="deleteCampaignResponse" type="tns:BaseCampaignResponse"/>


			<xs:element name="getCampaignRequest">
				<xs:annotation>
					<xs:documentation>
					</xs:documentation>
					<xs:appinfo>
						<Summary>
							Returns the ad campaign data for the campaign ID you specify.
						</Summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="campaignId" type="xs:long">
							<xs:annotation>
								<xs:documentation>
									Specify the ad campaign you want to retrieve using the campaignId number
									that eBay returned when you created the ad campaign.
									&lt;br /&gt;&lt;br /&gt;
									This call does not return category campaigns nor deleted campaigns. eBay
									returns an error if you enter the campaignId of a deleted campaign or the
									ID of an ad campaign that uses categories instead of keyword matches. [??]
								</xs:documentation>
								<xs:appinfo>
									<CallInfo>
										<CallName>getCampaign</CallName>
										<RequiredInput>Yes</RequiredInput>
									</CallInfo>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="getCampaignResponse" type="tns:CampaignResponse"/>


			<xs:element name="getCampaignsRequest">
				<xs:annotation>
					<xs:documentation>
						Returns all the campaigns associated with the user. Each campaign
						is returned in its own container. There is no input parameter for
						this call. The user is determined by [How ??].
					</xs:documentation>
					<xs:appinfo>
						<Summary>
							Returns details on all the ad campaigns associated with the
							respective user.
						</Summary>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="getCampaignsResponse" type="tns:BulkCampaignResponse"/>


			<xs:element name="updateCampaignRequest">
				<xs:annotation>
					<xs:documentation>
						Update the campaign identified by the specified campaign ID. Campaign
						fields that can be updated are: [??]
						&lt;br /&gt;&lt;br /&gt;
						Note that you cannot update a category campaign. [??]
					</xs:documentation>
					<xs:appinfo>
						<Summary>
							Update the campaign identified by the specified campaign ID.
						</Summary>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="campaign" type="tns:Campaign">
							<xs:annotation>
								<xs:documentation>
									Specify the ad campaign you want to update using the campaignId number
									that eBay returned when you created the ad campaign.
									&lt;br /&gt; &lt;br /&gt;
									This call takes a Campaign type as input. Specify values for the fields
									that you want to update. You can modify the following fields: [??]
								</xs:documentation>
								<xs:appinfo>
									<CallInfo>
										<CallName>updateCampaign</CallName>
										<RequiredInput>Yes</RequiredInput>
									</CallInfo>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="updateCampaignResponse" type="tns:BaseCampaignResponse"/>


			<!--  common types  -->
			<xs:element name="getVersionResponse" nillable="true" type="tns:GetVersionResponse"/>


			<xs:complexType name="GetVersionResponse">
				<xs:annotation>
					<xs:documentation>
					</xs:documentation>
					<xs:appinfo>
						<CallInfo>
							<CallName/>
							<RequiredInput>Yes</RequiredInput>
						</CallInfo>
						<CallInfo>
							<CallName/>
							<Returned>Always</Returned>
						</CallInfo>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseServiceResponse">
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>


			<xs:complexType abstract="true" name="BaseAdCommerceResponse">
				<xs:annotation>
					<xs:documentation>
						Base response container for all AdCommerce operations.
